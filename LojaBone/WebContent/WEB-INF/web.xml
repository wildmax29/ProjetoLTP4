<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>LojaBone</display-name>
  <welcome-file-list>
    <welcome-file>/JSP/Login.jsp</welcome-file>
  </welcome-file-list>
<servlet>
	<servlet-name>Cliente</servlet-name>
	<servlet-class>br.com.Servlet.ClienteServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>Cliente</servlet-name>
	<url-pattern>/cliente</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>Bone</servlet-name>
	<servlet-class>br.com.Servlet.BoneServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>Bone</servlet-name>
	<url-pattern>/bone</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>Login</servlet-name>
	<servlet-class>br.com.Servlet.LoginServlet</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>Login</servlet-name>
	<url-pattern>/Login</url-pattern>
</servlet-mapping>
</web-app>